<?xml version="1.0" encoding="UTF-8"?>
<launch>

      <!-- Wire static transform from the world to velodyne frame -->
    <node
        pkg="tf2_ros"
        type="static_transform_publisher"
        name="world_velodyne_tf"
        output="screen"
        args="-0.0497178 0.05852865 -0.20228732 -1.1521994669428535 -1.4757474440516494 2.7501533797180113 world velodyne" />

  <node
    pkg="lidar_detection"
    type="lidar_detection.py"
    name="lidar_detection"
    output="screen" />

   <node
     pkg="lidar_detection"
     type="lidar_3d_to_2d.py"
     name="lidar_3d_to_2d"
     output="screen" />

</launch>

